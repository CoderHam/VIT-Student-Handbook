<div ng-controller="header">
    <header class="VIT">
        <div class="navbar-fixed">
            <nav>
                <div class="nav-wrapper VIT">
                    <ul class="left">
                        <li><a href="#/"   class="white-text waves-effect waves-light">VIT Student Handbook</a></li>
                    </ul>
                    <div class="input-field left">
                        <form ng-submit="searchArt()" onsubmit="window.location.href = '#/results';">
                            <input id="search" placeholder="Search..." type="search" ng-model="search">
                            <label for="search">
                                <button type="submit"><i class="mdi-action-search"></i></button>
                            </label>
                            <i class="mdi-navigation-close"></i>
                        </form>
                    </div>
                    <ul class="right">
                        <li><a id="categories" href="#" data-activates="slide-out"
                               class="button-collapse white-text waves-effect waves-light">Categories</a></li>
                        <li><a id="login" href="#modal1" class="modal-trigger white-text waves-effect waves-light"
                               onclick="setDynamicElements()">Login</a></li>
                    </ul>
                </div>
            </nav>
        </div>
    </header>
    <div id="modal1" class="modal">
        <div class="modal-content">
            <h4 class="center-align teal-text text-darken-3">Login</h4>

            <div class="input-field center-align">
                <i class="mdi-action-account-box prefix"></i>
                <input id="reg_no" type="text" class="validate">
                <label for="reg_no" class="text-darken-4">Registration Number</label>
            </div>
            <div class="remember">
                <div class="right">
                    <input type="checkbox" id="rem"/>
                    <label for="rem">Remember</label>
                </div>
            </div>
        </div>
        <div class="modal-footer">
            <a href="#!" class=" modal-action modal-close waves-effect waves-blue btn">Login</a>
        </div>
    </div>
